# Ping
ansible -m ping -e 'ansible_python_interpreter=/usr/bin/python3' all

# List Hosts
ansible -i test --list-hosts all
ansible -i test --list-hosts webserver
ansible -i test --list-hosts loadbalancer
ansible -i test --list-hosts "app*"
ansible -i test --list-hosts database:control
ansible -i test --list-hosts webserver[0]


# Command Module
ansible -a "hostname" all